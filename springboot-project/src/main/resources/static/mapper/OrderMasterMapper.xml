<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imooc.repository.OrderMasterRepository">
    <resultMap id="BaseResultMap" type="com.imooc.dataobject.OrderMaster">
        <result column="orderId" jdbcType="VARCHAR" property="orderId" />
        <result column="buyer_name" jdbcType="VARCHAR" property="buyerName" />
        <result column="buyer_phone" jdbcType="VARCHAR" property="buyerPhone" />
        <result column="buyer_address" jdbcType="VARCHAR" property="buyerAddress" />
        <result column="buyer_openid" jdbcType="VARCHAR" property="buyerOpenid" />
        <result column="order_amount" jdbcType="VARCHAR" property="orderAmount" />
        <result column="order_status" jdbcType="VARCHAR" property="orderStatus" />
        <result column="pay_status" jdbcType="VARCHAR" property="payStatus" />
        <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
        <result column="update_time" jdbcType="TIMESTAMP" property="updateTime" />
    </resultMap>
    <select id="findAlls" resultMap="BaseResultMap">
        select *
        from order_master
    </select>

</mapper>
